                            </Path.ToolTip>
                        </Path>
                        <!--#endregion-->
                        <!--#region  Address-->
                        <TextBox x:Name="tb_address"  Grid.Row="2"  Text="{Binding address, Mode=OneWay}"    Padding="10,0,5,0" 
