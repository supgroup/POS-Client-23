                                            <ColumnDefinition Width="*"/>
                                        </Grid.ColumnDefinitions>
                                        <TextBox x:Name="tb_details"  Text="{Binding details, Mode=OneWay}"  Padding="10,0,5,0"
                                    Style="{StaticResource MaterialDesignFloatingHintTextBox}" 
                                 Grid.Row="3"  BorderThickness="1"  MaxLength="100" 
                                 materialDesign:HintAssist.Hint="Details..."  
                           Foreground="#4E4E4E"  TextBlock.FontSize="12" Background="#f8f8f8"
                                 BorderBrush="#f8f8f8" materialDesign:TextFieldAssist.CharacterCounterStyle="{Binding}"
                  Height="40" Margin="10,5" />
                                        <Path Fill="#E81123" Stretch="Fill" Margin="5,5,15,5"
                              HorizontalAlignment="Right" Grid.Column="2"  Visibility="Collapsed"   
                         VerticalAlignment="Center" Width="15"  Height="15"
                              Data="M12,2 C6.48,2 2,6.48 2,12 C2,17.52 6.48,22 12,22 C17.52,22 22,17.52 22,12 C22,6.48 17.52,2 12,2 Z M12,13 C11.45,13 11,12.55 11,12 L11,8 C11,7.45 11.45,7 12,7 C12.55,7 13,7.45 13,8 L13,12 C13,12.55 12.55,13 12,13 Z M13,17 L11,17 L11,15 L13,15 L13,17 Z">
                                            <Path.ToolTip>
                                                <ToolTip x:Name="tt_errorDetails" Content="Name Not Validate"  Style="{StaticResource ToolTipError}"/>
                                            </Path.ToolTip>
                                        </Path>
                                    </Grid>
                                </Grid>
                                <Grid Grid.Row="2" >
                                    <Grid.RowDefinitions >
                                        <RowDefinition Height="auto"/>
                                        <RowDefinition Height="auto"/>
                                        <RowDefinition Height="auto"/>
                                    </Grid.RowDefinitions>
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="*"/>
                                    </Grid.ColumnDefinitions>
                                    <Grid Grid.ColumnSpan="2">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="auto"/>
                                            <ColumnDefinition Width="*"/>
                                        </Grid.ColumnDefinitions>
                                        <TextBlock x:Name="txt_secondaryInformation" Text="Secondary Infomation" Foreground="#737373"
                                 FontSize="10"      VerticalAlignment="Center"
                                       HorizontalAlignment="Left" Margin="5,5,0,5"/>
                                        <Separator Background="#999999"   Width="260"  Margin="5,0,5,0"
                                      Grid.Column="1" VerticalAlignment="Center" />
                                    </Grid>
                                    <Grid  Grid.Row="1">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="*"/>
                                        </Grid.ColumnDefinitions>
                                        <!--LostFocus="Cb_categorie_LostFocus"-->
                                        <ComboBox  x:Name="cb_categorie" Grid.Row="2"
                                                   Padding="10,0,5,0"  TextBlock.Foreground="#4E4E4E"
                                                Style="{StaticResource MaterialDesignFloatingHintComboBox}" 
                                                   materialDesign:HintAssist.Hint="Select Categorie "  
                                                   TextBlock.FontSize="12"   Background="#f8f8f8" 
                                                   Grid.Column="1" Height="40"   Margin="10,5" 
                                                   BorderBrush="#737373" BorderThickness="0"
                                                   SelectionChanged="Cb_categorie_SelectionChanged"
                                                   LostFocus="Tb_validateEmptyLostFocus" KeyUp="Cb_categorie_KeyUp"/>
                                        <Path Fill="#E81123" x:Name="p_errorCategorie" Stretch="Fill" Margin="5,5,15,5"
                              HorizontalAlignment="Right" Grid.Column="2"  Visibility="Collapsed"   
                         VerticalAlignment="Center" Width="15"  Height="15"
                              Data="M12,2 C6.48,2 2,6.48 2,12 C2,17.52 6.48,22 12,22 C17.52,22 22,17.52 22,12 C22,6.48 17.52,2 12,2 Z M12,13 C11.45,13 11,12.55 11,12 L11,8 C11,7.45 11.45,7 12,7 C12.55,7 13,7.45 13,8 L13,12 C13,12.55 12.55,13 12,13 Z M13,17 L11,17 L11,15 L13,15 L13,17 Z">
                                            <Path.ToolTip>
                                                <ToolTip  x:Name="tt_errorCategorie" Content="Name Not Validate"  Style="{StaticResource ToolTipError}"/>
                                            </Path.ToolTip>
                                        </Path>
                                    </Grid>

                                    <Grid x:Name="grid_taxes" Grid.Row="2" >
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="*"/>
                                            <ColumnDefinition Width="auto"/>
                                        </Grid.ColumnDefinitions>
                                        <!--PreviewTextInput="DecimalValidationTextBox"  PreviewKeyDown="tb_upperLimit_PreviewKeyDown"-->
                                        <!--LostFocus="Tb_taxes_LostFocus"-->
                                        <TextBox x:Name="tb_taxes"  Text="{Binding taxes, Mode=OneWay}"
                                                 Grid.Row="2" Padding="10,0,5,0" MaxLength="20" 
                                                 materialDesign:TextFieldAssist.CharacterCounterStyle="{Binding}"   
                                    Style="{StaticResource MaterialDesignFloatingHintTextBox}" 
                                                 BorderThickness="1"  materialDesign:HintAssist.Hint="Tax..."  
                                                 Foreground="#4E4E4E"  TextBlock.FontSize="12" Background="#f8f8f8"
                                                 BorderBrush="#f8f8f8" TextChanged="Tb_validateEmptyTextChange"
                                                 Grid.Column="0"  Height="40"  Margin="10,5" 
                                                 PreviewKeyDown="Tb_PreventSpaces" 
                                                 PreviewTextInput="Tb_decimal_PreviewTextInput" 
                                                 LostFocus="Tb_validateEmptyLostFocus"/>
                                        <Path x:Name="p_errorTaxes" Fill="#E81123" Stretch="Fill" Margin="5,5,15,5"
                              HorizontalAlignment="Right" Grid.Column="0"  Visibility="Collapsed"   
                         VerticalAlignment="Center" Width="15"  Height="15"
                              Data="M12,2 C6.48,2 2,6.48 2,12 C2,17.52 6.48,22 12,22 C17.52,22 22,17.52 22,12 C22,6.48 17.52,2 12,2 Z M12,13 C11.45,13 11,12.55 11,12 L11,8 C11,7.45 11.45,7 12,7 C12.55,7 13,7.45 13,8 L13,12 C13,12.55 12.55,13 12,13 Z M13,17 L11,17 L11,15 L13,15 L13,17 Z">
                                            <Path.ToolTip>
                                                <ToolTip x:Name="tt_errorTaxes" Content="Name Not Validate"  Style="{StaticResource ToolTipError}"/>
                                            </Path.ToolTip>
                                        </Path>
                                        <materialDesign:PackIcon Foreground="#6e6e6e" Kind="PercentOutline" 
                                                      Grid.Column="3" Margin="0 5 10 5"
                                                     HorizontalAlignment="Center" VerticalAlignment="Center"
                                                     Width="20" Height="20"/>
                                    </Grid>
                                </Grid>
                                <Grid  Grid.Row="3" >
                                    <Grid.RowDefinitions >
                                        <RowDefinition Height="auto"/>
                                        <RowDefinition Height="auto"/>
                                        <RowDefinition Height="auto"/>
                                        <RowDefinition Height="auto"/>
                                        <RowDefinition Height="auto"/>
                                        <RowDefinition Height="auto"/>
                                    </Grid.RowDefinitions>
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="*"/>
                                    </Grid.ColumnDefinitions>
                                    <Grid Grid.ColumnSpan="2">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="auto"/>
                                            <ColumnDefinition Width="*"/>
                                        </Grid.ColumnDefinitions>
                                        <TextBlock  Text="Barcode" Foreground="#737373"
                                 FontSize="10"      VerticalAlignment="Center" x:Name="txt_barcode"
                                       HorizontalAlignment="Left" Margin="5,5,0,5"/>
                                        <Separator Background="#999999"   Width="360"  Margin="5,0,5,0"
                                      Grid.Column="1" VerticalAlignment="Center" />
                                    </Grid>
                                    <Grid  Grid.Row="1" Grid.ColumnSpan="2"  >
                                        <TextBox x:Name="tb_cost"   Text="{Binding avgPurchasePrice, Mode=OneWay}"
                                    Style="{StaticResource MaterialDesignFloatingHintTextBox}" 
                                                 Grid.Row="1" Padding="10,0,5,0" 
                                                 BorderThickness="1"  materialDesign:HintAssist.Hint="Cost..."   
                                                 Foreground="#4E4E4E"  TextBlock.FontSize="12" Background="#f8f8f8"
                                                 BorderBrush="AliceBlue" 
                                                 Height="40"  Margin="5,5" KeyDown="tb_barcode_Generate" TextChanged="Tb_validateEmptyTextChange"
                                                 PreviewKeyDown="Tb_PreventSpaces" 
                                                 PreviewTextInput="Tb_decimal_PreviewTextInput" 
                                                 LostFocus="Tb_validateEmptyLostFocus"/>
                                        <Path Fill="#E81123" Stretch="Fill" Margin="5,5,15,5" x:Name="p_errorCost"
                                              HorizontalAlignment="Right" Grid.Column="2"  Visibility="Collapsed"   
                                              VerticalAlignment="Center" Width="15"  Height="15"
                                              Data="M12,2 C6.48,2 2,6.48 2,12 C2,17.52 6.48,22 12,22 C17.52,22 22,17.52 22,12 C22,6.48 17.52,2 12,2 Z M12,13 C11.45,13 11,12.55 11,12 L11,8 C11,7.45 11.45,7 12,7 C12.55,7 13,7.45 13,8 L13,12 C13,12.55 12.55,13 12,13 Z M13,17 L11,17 L11,15 L13,15 L13,17 Z">
                                            <Path.ToolTip>
                                                <ToolTip x:Name="tt_errorCost"    Style="{StaticResource ToolTipError}"/>
                                            </Path.ToolTip>
                                        </Path>
                                    </Grid>
                                    <Grid  Grid.Row="2" Grid.ColumnSpan="2"  >
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="*"/>
                                        </Grid.ColumnDefinitions>
                                        <TextBox x:Name="tb_price"   Text="{Binding price, Mode=OneWay}"
                                    Style="{StaticResource MaterialDesignFloatingHintTextBox}" 
                                                 Grid.Row="1" Padding="10,0,5,0" 
                                                 BorderThickness="1"  materialDesign:HintAssist.Hint="Price..."   
                                                 Foreground="#4E4E4E"  TextBlock.FontSize="12" Background="#f8f8f8"
                                                 BorderBrush="AliceBlue" 
                                                 Height="40"  Margin="5,5" KeyDown="tb_barcode_Generate" TextChanged="Tb_validateEmptyTextChange"
                                                 PreviewKeyDown="Tb_PreventSpaces" 
                                                 PreviewTextInput="Tb_decimal_PreviewTextInput" 
                                                 LostFocus="Tb_validateEmptyLostFocus"/>
                                        <Path Fill="#E81123" Stretch="Fill" Margin="5,5,15,5" x:Name="p_errorPrice"
                                              HorizontalAlignment="Right" Grid.Column="2"  Visibility="Collapsed"   
                                              VerticalAlignment="Center" Width="15"  Height="15"
                                              Data="M12,2 C6.48,2 2,6.48 2,12 C2,17.52 6.48,22 12,22 C17.52,22 22,17.52 22,12 C22,6.48 17.52,2 12,2 Z M12,13 C11.45,13 11,12.55 11,12 L11,8 C11,7.45 11.45,7 12,7 C12.55,7 13,7.45 13,8 L13,12 C13,12.55 12.55,13 12,13 Z M13,17 L11,17 L11,15 L13,15 L13,17 Z">
                                            <Path.ToolTip>
                                                <ToolTip x:Name="tt_errorPrice"  Content="Name Not Validate"  Style="{StaticResource ToolTipError}"/>
                                            </Path.ToolTip>
                                        </Path>
                                    </Grid>
                                    <Grid  Grid.Row="3" Grid.ColumnSpan="2"  >
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="*"/>
                                        </Grid.ColumnDefinitions>
                                        <TextBox x:Name="tb_barcode"   Text="{Binding test, Mode=OneWay}"
                                    Style="{StaticResource MaterialDesignFloatingHintTextBox}" 
                                                 Grid.Row="1" Padding="10,0,5,0" 
                                                 BorderThickness="1"  materialDesign:HintAssist.Hint="Barcode..."   
                                                 Foreground="#4E4E4E"  TextBlock.FontSize="12" Background="#f8f8f8"
                                                 BorderBrush="AliceBlue" IsReadOnly="True"
                                                 Height="40"   Margin="5,5" 
                                                 KeyDown="tb_barcode_Generate" 
                                                 TextChanged="tb_barcode_TextChanged"/>
                                        <Path Fill="#E81123" Stretch="Fill" Margin="5,5,15,5"
                              HorizontalAlignment="Right" Grid.Column="2"  Visibility="Collapsed"   
                         VerticalAlignment="Center" Width="15"  Height="15"
                              Data="M12,2 C6.48,2 2,6.48 2,12 C2,17.52 6.48,22 12,22 C17.52,22 22,17.52 22,12 C22,6.48 17.52,2 12,2 Z M12,13 C11.45,13 11,12.55 11,12 L11,8 C11,7.45 11.45,7 12,7 C12.55,7 13,7.45 13,8 L13,12 C13,12.55 12.55,13 12,13 Z M13,17 L11,17 L11,15 L13,15 L13,17 Z">
                                            <Path.ToolTip>
                                                <ToolTip  Content="Name Not Validate"  Style="{StaticResource ToolTipError}"/>
                                            </Path.ToolTip>
                                        </Path>
                                    </Grid>
                                    <Image x:Name="img_barcode" Margin="50,5"  Height="50"  Grid.Row="4"
                               Stretch="Fill" Grid.ColumnSpan="2"   />

                                    <Separator Grid.Row="5" Grid.ColumnSpan="2"  Visibility="Visible"
                            Background="#999999"    Margin="10,0,10,0"
                         VerticalAlignment="Center" />
                                </Grid>
                            </Grid>
                            </ScrollViewer>

                            <Grid Grid.Row="2" Margin="5,1" >
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="*"/>
                                    <ColumnDefinition Width="*"/>
                                    <ColumnDefinition Width="*"/>
                                </Grid.ColumnDefinitions>
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="*" />
                                    <RowDefinition Height="*" />
                                    <RowDefinition Height="*" />
                                </Grid.RowDefinitions>
                                <Border Background="White" Grid.ColumnSpan="3"  
                            Height="1"  BorderBrush="{StaticResource  LightGrey}" 
                    BorderThickness="1" Opacity="1" Margin="5,5,5,5"  />
                                <Grid  Grid.Row="1" Grid.ColumnSpan="4">
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="*"/>
                                        <ColumnDefinition Width="*"/>
                                    </Grid.ColumnDefinitions>
                                    <Button Grid.Column="0" 
                                BorderBrush="{x:Null}" Padding="0" 
                                Name="btn_prices"  Margin="5 5 2.5 5 " Height="auto"
                                Click="Btn_prices_Click"
                                Background="{x:Null}" VerticalAlignment="Top" IsEnabled="True"  >
                                        <Grid>
                                            <Grid.RowDefinitions>
                                                <RowDefinition Height="*"/>
                                                <RowDefinition Height="auto"/>
                                            </Grid.RowDefinitions>
                                            <Path Fill="{StaticResource SecondColor}" Stretch="Fill"  
                           Width="35" Height="35" FlowDirection="LeftToRight"
                           Data="{StaticResource dolarsList}"/>
                                            <TextBlock x:Name="txt_pricesButton"
                                    Text="prices"  Grid.Row="1" 
                                                    FontWeight="Regular" FontSize="10" TextAlignment="Center"
                                                    Foreground="{StaticResource MainColor}"
                                                     HorizontalAlignment="Center" TextWrapping="Wrap"/>
                                        </Grid>
                                    </Button>
                                </Grid>

                                <!--<Button x:Name="btn_prices" Click="Btn_prices_Click"
                                Grid.Row="0" Grid.ColumnSpan="3"
                              Background="#4E4E4E" BorderBrush="{x:Null}" 
                                Margin="1,1"    Content="prices" />-->
                                <Border Grid.Row="2">
                                    <Button x:Name="btn_add" TabIndex="10"
                          Background="#4E4E4E" BorderBrush="{x:Null}"
                    Margin="1,1" Click="Btn_add_Click" >
                                        <StackPanel Orientation="Horizontal" HorizontalAlignment="Center" VerticalAlignment="Center">
                                            <materialDesign:PackIcon Name="txt_add_Icon" Kind="PlusThick" VerticalAlignment="Center" Width="15"  Height="15" Margin="0,1,2,0">

                                            </materialDesign:PackIcon>
                                            <TextBlock Visibility="Collapsed" Name="txt_addButton" Text="Add" VerticalAlignment="Center">

                                            </TextBlock>

                                        </StackPanel>
                                    </Button>
                                    <Border.ToolTip>
                                        <ToolTip x:Name="tt_add_Button" Content="Add"
                                    Placement="left" 
                                    HorizontalOffset="15"
                                    PlacementRectangle="50,0,0,0"
                                    VerticalOffset="30"/>
                                    </Border.ToolTip>
                                </Border>
                                <Border Grid.Row="2" Grid.Column="1">
                                    <Button x:Name="btn_update"  TabIndex="11"
                        Background="#4E4E4E" BorderBrush="{x:Null}"
                   Margin="1,1"  Click="Btn_update_Click" >
                                        <StackPanel Orientation="Horizontal" HorizontalAlignment="Center" VerticalAlignment="Center">
                                            <materialDesign:PackIcon Name="txt_update_Icon" Kind="Edit" VerticalAlignment="Center" Width="15" Height="15" Margin="0,1,2,0">

                                            </materialDesign:PackIcon>
                                            <TextBlock Visibility="Collapsed" Name="txt_updateButton" Text="Update" VerticalAlignment="Center">

                                            </TextBlock>

                                        </StackPanel>
                                    </Button>
                                    <Border.ToolTip>

                                        <ToolTip x:Name="tt_update_Button" Content="Update"
                                    Placement="left" 
                                    HorizontalOffset="15"
                                    PlacementRectangle="50,0,0,0"
                                    VerticalOffset="30"/>

                                    </Border.ToolTip>
                                </Border>
                                <Border Grid.Row="2" Grid.Column="2">
                                    <Button x:Name="btn_delete"  TabIndex="12"
                        Background="#4E4E4E" BorderBrush="{x:Null}"
                    Margin="1,1"  Click="Btn_delete_Click" >
                                        <StackPanel HorizontalAlignment="Center" Orientation="Horizontal" VerticalAlignment="Center">
                                            <materialDesign:PackIcon Name="txt_delete_Icon" Kind="Delete" VerticalAlignment="Center" Width="15" Height="15" Margin="0,1,2,0">

                                            </materialDesign:PackIcon>
                                            <TextBlock Visibility="Collapsed" Name="txt_deleteButton" Width="auto" Text="Delete" VerticalAlignment="Center">

                                            </TextBlock>
                                        </StackPanel>
                                    </Button>
                                    <Border.ToolTip>
                                        <ToolTip x:Name="tt_delete_Button" Content="delete"
                                    Placement="left" 
                                    HorizontalOffset="15"
                                    PlacementRectangle="50,0,0,0"
                                    VerticalOffset="30"/>
                                    </Border.ToolTip>
                                </Border>
                            </Grid>
                        </Grid>
                        
                    </Grid>
                </Border>
            </Grid>
        </Border>
    </Grid>
</UserControl>
