                <RowDefinition Height="auto"></RowDefinition>
                <RowDefinition Height="*"></RowDefinition>
            </Grid.RowDefinitions>

            <Border Grid.Row="0"  Background="White" Margin="5,0,5,5" CornerRadius="7">
                <Grid Margin="5,10,5,10">
                    <lvc:CartesianChart Name="cartesianChart" LegendLocation="Bottom">
                        <lvc:CartesianChart.AxisX>
                            <lvc:Axis  x:Name="axcolumn">
                                <lvc:Axis.Separator>
                                    <lvc:Separator Step="1"/>
                                </lvc:Axis.Separator>
                            </lvc:Axis>
                        </lvc:CartesianChart.AxisX>
                    </lvc:CartesianChart>
                    <Button   Background="{x:Null}"    Margin="5,0,5,0"
                          Padding="0" Height="35"  
                              Tag="cartesianChart"
                              HorizontalAlignment="Right" 
                              VerticalAlignment="Top"
                          BorderBrush="{x:Null}" Click="Btn_printChart_Click">
                        <materialDesign:PackIcon Foreground="#313131" Kind="Printer"  
                              HorizontalAlignment="Center" 
                             VerticalAlignment="Center"
                            Margin="0 -12 0 0"
                      Width="22" Height="22">
                            <materialDesign:PackIcon.ToolTip>
                                <ToolTip x:Name="tt_print1" Content="Print"
                                              Placement="left" 
                                              HorizontalOffset="5"
                                              PlacementRectangle="50,0,0,0"
                                              VerticalOffset="-10"/>
                            </materialDesign:PackIcon.ToolTip>
                        </materialDesign:PackIcon>
                    </Button>
                </Grid>
            </Border>
            <GridSplitter Grid.Row="1" Background="Transparent" HorizontalAlignment="Stretch" Height="5" />

            <Border Grid.Row="2" Background="White" Margin="5,5,5,0" CornerRadius="7">
                <Grid Margin="5,10,5,10">

                    <lvc:CartesianChart Name="rowChart" LegendLocation="Bottom"  >
                    <lvc:CartesianChart.AxisX>
                        <lvc:Axis  x:Name="MyAxis">
                            <lvc:Axis.Separator>
                                <lvc:Separator Step="1"/>
                            </lvc:Axis.Separator>
                        </lvc:Axis>
                    </lvc:CartesianChart.AxisX>
                </lvc:CartesianChart>
                    <Button   Background="{x:Null}"    Margin="5,0,5,0"
                          Padding="0" Height="35"  
                              Tag="rowChart"
                              HorizontalAlignment="Right" 
                              VerticalAlignment="Top"
                          BorderBrush="{x:Null}" Click="Btn_printChart_Click">
                        <materialDesign:PackIcon Foreground="#313131" Kind="Printer"  
                              HorizontalAlignment="Center" 
                             VerticalAlignment="Center"
                            Margin="0 -12 0 0"
                      Width="22" Height="22">
                            <materialDesign:PackIcon.ToolTip>
                                <ToolTip x:Name="tt_print2" Content="Print"
                                              Placement="left" 
                                              HorizontalOffset="5"
                                              PlacementRectangle="50,0,0,0"
                                              VerticalOffset="-10"/>
                            </materialDesign:PackIcon.ToolTip>
                        </materialDesign:PackIcon>
                    </Button>
                </Grid>

            </Border>

        </Grid>
    </Grid>
</UserControl>
