<UserControl x:Class="POS.View.reports.uc_paymentsReport"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:lvc="clr-namespace:LiveCharts.Wpf;assembly=LiveCharts.Wpf"
             xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes"
             xmlns:local="clr-namespace:POS.View.reports"
             mc:Ignorable="d" Tag="paymentsReport"
             d:DesignHeight="450"
             d:DesignWidth="800"
             Loaded="UserControl_Loaded" Unloaded="UserControl_Unloaded">

    <!--Main Grid-->
    <Grid Margin="0,15,15,15" x:Name="grid_main">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="2*" ></ColumnDefinition>
            <ColumnDefinition Width="auto" ></ColumnDefinition>
            <ColumnDefinition x:Name="col_reportChart" Width="1*" ></ColumnDefinition>
        </Grid.ColumnDefinitions>

        <!--Left Grid-->
        <Grid Grid.Column="0">
            <Grid.RowDefinitions>
                <RowDefinition Height="auto"></RowDefinition>
                <RowDefinition Height="50"></RowDefinition>
                <RowDefinition Height="*"></RowDefinition>
                <RowDefinition Height="auto"/>
                <RowDefinition Height="50"/>
            </Grid.RowDefinitions>

            <!--First Grid Grid-->
            <Grid Margin="5,0,0,0">
                <Grid.RowDefinitions>
                    <RowDefinition Height="*"/>
                    <RowDefinition Height="auto"/>
                </Grid.RowDefinitions>

                <Border CornerRadius="7,7,0,0"
                        Height="52"
                        Grid.Row="0"
                        Margin="0,0,5,0"
                        BorderBrush="{x:Null}"
                        Background="#4E4E4E">

                    <!--Grid Tab Control-->
                    <Grid    x:Name="grid_tabControl">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="auto"/>
                            <ColumnDefinition Width="auto"/>
                            <ColumnDefinition Width="auto"/>
                            <ColumnDefinition Width="auto"/>
                            <ColumnDefinition Width="auto"/>
                            <ColumnDefinition Width="auto"/>
                            <ColumnDefinition Width="auto"/>
                            <ColumnDefinition Width="auto"/>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="auto"/>
                        </Grid.ColumnDefinitions>

                        <Border x:Name="bdr_vendor"
                                Grid.Column="0"
                                Background="White"
                                BorderThickness="0"
                                Height="40"
                                Width="60"
                                VerticalAlignment="Bottom"
                                CornerRadius="7,7,0,0"
                                BorderBrush="Transparent" 
                                Margin="10,5,0,-1">

                            <Button x:Name="btn_vendor" Tag="vendors"
                                    Background="Transparent"  
                                    materialDesign:RippleAssist.Feedback="Transparent"
                                    BorderBrush="Transparent"
                                    Height="auto" Click="Btn_vendor_Click"
                                    >
                                <Button.Triggers>
                                    <EventTrigger x:Name="trig" RoutedEvent="Button.Click">
                                        <BeginStoryboard>
                                            <Storyboard>
                                                <DoubleAnimation
                        Storyboard.TargetName="bdrMain"
                        Storyboard.TargetProperty="Opacity"
                        From="0" To="1"
                        Duration="0:0:1"/>
                                            </Storyboard>
                                        </BeginStoryboard>
                                    </EventTrigger>
                                </Button.Triggers>
                                <Path Name="path_vendor"
                                      Fill="#4E4E4E"
                                      Stretch="Fill" FlowDirection="LeftToRight"
                                      VerticalAlignment="Top"
                                      HorizontalAlignment="Center"
                                      Margin="0,-1,0,0" 
                                      Width="25"
                                      Height="25"   
                                      Data="{StaticResource vendor}"  >
                                    <Path.ToolTip>
                                        <ToolTip x:Name="tt_vendor"
                                                 Content="Vendor"
                                                 Placement="left" 
                                                 HorizontalOffset="35"
                                                 PlacementRectangle="50,0,0,0"
                                                 VerticalOffset="-10"/>
                                    </Path.ToolTip>
                                </Path>
                            </Button>
                        </Border>

                        <Border x:Name="bdr_customer"
                                Grid.Column="1"
                                Background="#4E4E4E"
                                BorderThickness="0"
                                Height="40"
                                Width="60"
                                VerticalAlignment="Bottom"
                                CornerRadius="7,7,0,0" 
                                BorderBrush="Transparent"
                                Margin="0,5,0,-1">

                            <Button x:Name="btn_customer" Tag="customers"
                                    Background="Transparent"  
                                    materialDesign:RippleAssist.Feedback="Transparent"
                                    BorderBrush="Transparent"
                                    Height="auto" Click="Btn_customer_Click" >
                                <Button.Triggers>
                                    <EventTrigger RoutedEvent="Button.Click">
                                        <BeginStoryboard>
                                            <Storyboard>
                                                <DoubleAnimation
                        Storyboard.TargetName="bdrMain"
                        Storyboard.TargetProperty="Opacity"
                        From="0" To="1"
                        Duration="0:0:1"/>
                                            </Storyboard>
                                        </BeginStoryboard>
                                    </EventTrigger>
                                </Button.Triggers>
                                <Path x:Name="path_customer" 
                                      Fill="White"
                                      Stretch="Fill"  FlowDirection="LeftToRight"
                                      VerticalAlignment="Top"
                                      Margin="0,-1,0,0"
                                      HorizontalAlignment="Center"
                                      Width="25" 
                                      Height="25"  
                                      Data="{StaticResource customer}"  >
                                    <Path.ToolTip>
                                        <ToolTip x:Name="tt_customer"
                                                 Content="Customer"
                                                 Placement="left" 
                                                 HorizontalOffset="35"
                                                 PlacementRectangle="50,0,0,0"
                                                 VerticalOffset="-10"/>
                                    </Path.ToolTip>
                                </Path>
                            </Button>
                        </Border>

                        <Border x:Name="bdr_user"
                                Grid.Column="2"
                                Background="#4E4E4E"
                                BorderThickness="0"
                                Height="40"
                                Width="60"
                                VerticalAlignment="Bottom"
                                CornerRadius="7,7,0,0"
                                BorderBrush="Transparent"
                                Margin="0,5,0,-1">

                            <Button x:Name="btn_user" Tag="user"
                                    Background="Transparent" 
                                    materialDesign:RippleAssist.Feedback="Transparent"
                                    BorderBrush="Transparent"
                                    Height="auto" Click="Btn_user_Click">
                                <Button.Triggers>
                                    <EventTrigger RoutedEvent="Button.Click">
                                        <BeginStoryboard>
                                            <Storyboard>
                                                <DoubleAnimation
                        Storyboard.TargetName="bdrMain"
                        Storyboard.TargetProperty="Opacity"
                        From="0" To="1"
                        Duration="0:0:1"/>
                                            </Storyboard>
                                        </BeginStoryboard>
                                    </EventTrigger>
                                </Button.Triggers>
                                <Path x:Name="path_user" 
                                      Fill="White"
                                      Stretch="Fill"  FlowDirection="LeftToRight"
                                      VerticalAlignment="Top"
                                      Margin="0,-1,0,0"
                                      HorizontalAlignment="Center"
                                      Width="25"
                                      Height="25" 
                                      Data="{StaticResource user}"  >
                                    <Path.ToolTip>
                                        <ToolTip x:Name="tt_user" Content="User"
                                              Placement="left" 
                                              HorizontalOffset="35"
                                              PlacementRectangle="50,0,0,0"
                                              VerticalOffset="-10"/>
                                    </Path.ToolTip>
                                </Path>
                            </Button>
                        </Border>

                        <Border Name="bdr_salary"
                                Grid.Column="3"
                                Background="#4E4E4E"
                                BorderThickness="0"
                                Height="40"
                                Width="60"
                                VerticalAlignment="Bottom"
                                CornerRadius="7,7,0,0"
                                BorderBrush="Transparent" 
                                Margin="0,5,0,-1">
                            <Button x:Name="btn_salary"  Tag="salary"
                                    Background="Transparent"  
                                    materialDesign:RippleAssist.Feedback="Transparent"
                                    BorderBrush="Transparent"
                                    Height="auto" Click="Btn_salary_Click" >
