	 M23.1,11.7H12.8v0.4h10.3V11.7z"  >
                                    <Path.ToolTip>
                                        <ToolTip x:Name="tt_order" Content="Orders"
                                              Placement="left" 
                                              HorizontalOffset="35"
                                              PlacementRectangle="50,0,0,0"
                                              VerticalOffset="-10"/>
                                    </Path.ToolTip>
                                </Path>
                            </Button>
                        </Border>
                        <Border x:Name="bdr_quotation" Grid.Column="2" Background="#4E4E4E" BorderThickness="0"  Height="40" Width="60" VerticalAlignment="Bottom" CornerRadius="7,7,0,0" BorderBrush="Transparent" Margin="0,5,0,-1">
                            <Button x:Name="btn_quotation"  Background="Transparent"  Tag="quotation"
                                    materialDesign:RippleAssist.Feedback="Transparent"
                                    BorderBrush="Transparent" Height="auto" Width="60" Click="Btn_quotation_Click">
                                <Button.Triggers>
                                    <EventTrigger RoutedEvent="Button.Click">
                                        <BeginStoryboard>
                                            <Storyboard>
                                                <DoubleAnimation
                        Storyboard.TargetName="bdrMain"
                        Storyboard.TargetProperty="Opacity"
                        From="0" To="1"
                        Duration="0:0:1"/>
                                            </Storyboard>
                                        </BeginStoryboard>
                                    </EventTrigger>
                                </Button.Triggers>
                                <Path Fill="White" Stretch="Fill"  VerticalAlignment="Top" Margin="0,-1,0,0" HorizontalAlignment="Center"
                                       Width="25" Height="25"    x:Name="path_quotation" FlowDirection="LeftToRight"
                                         Data="{StaticResource dolarsList}"  >
                                    <Path.ToolTip>
                                        <ToolTip x:Name="tt_quotation" Content="Quotations"
                                              Placement="left" 
                                              HorizontalOffset="35"
                                              PlacementRectangle="50,0,0,0"
                                              VerticalOffset="-10"/>
                                    </Path.ToolTip>
                                </Path>
                            </Button>
                        </Border>
                        <Border Grid.Column="10"  CornerRadius="7">
                            <TextBlock  x:Name="txt_tabTitle"  Foreground="White"   
                              VerticalAlignment="Center"      FontSize="16"     Margin="5 5 10 5"/>
                        </Border>
                        

                    </Grid>
                </Border>

                <Border CornerRadius="0,0,7,7" 
                        Grid.Row="1" Margin="0,0,5,10"
                        BorderBrush="{x:Null}"
                        Background="White" Height="auto">
                    <Border BorderThickness="15,0,15,0" x:Name="bdrMain" Height="auto" Background="Transparent">
                        <ScrollViewer VerticalScrollBarVisibility="Disabled" HorizontalScrollBarVisibility="Auto" >
                            <Grid x:Name="grid_father">
                                <Grid Grid.Row="1" x:Name="grid_invoice" Margin="10,0,10,10"  Visibility="Visible">
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="auto"></RowDefinition>
                                        <RowDefinition Height="auto"></RowDefinition>
                                        <RowDefinition Height="auto"></RowDefinition>

                                    </Grid.RowDefinitions>
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="auto"/>
                                        <ColumnDefinition Width="auto"/>
                                        <ColumnDefinition Width="*"/>
                                    </Grid.ColumnDefinitions>

                                    <DockPanel Grid.ColumnSpan="2" LastChildFill="False" Visibility="Visible">
                                        <!--Checked="search_Checking" Unchecked="chk_uncheck"-->
                                        <CheckBox x:Name="rad_invoice"   
                                                          Background="{StaticResource MainColorBlue}"
                                                          Content="Invoice"
                                                          Style="{StaticResource MaterialDesignFilterChipPrimaryOutlineCheckBox}"
                                                          Checked="RefreshViewCheckbox" Unchecked="RefreshViewCheckbox"/>

                                        <CheckBox x:Name="rad_return"  
                                                          Background="{StaticResource MainColorBlue}"
                                                          Content="Return"
                                                          Style="{StaticResource MaterialDesignFilterChipPrimaryOutlineCheckBox}"
                                                          Checked="RefreshViewCheckbox" Unchecked="RefreshViewCheckbox"/>
                                        <CheckBox x:Name="rad_draft"  
                                                          Background="{StaticResource MainColorBlue}"
                                                          Content="Draft"
                                                          Style="{StaticResource MaterialDesignFilterChipPrimaryOutlineCheckBox}"
                                                          Checked="RefreshViewCheckbox" Unchecked="RefreshViewCheckbox"/>

                                    </DockPanel>
                                    <!--<DockPanel Grid.ColumnSpan="2" LastChildFill="False">
                                        --><!--<CheckBox x:Name="chk_invoice" Content="Invoice" Margin="5" Foreground="{StaticResource MainColorGrey}" Checked="RefreshViewCheckbox" Unchecked="RefreshViewCheckbox"></CheckBox>
                                        <CheckBox x:Name="chk_return" Content="Return invoice" Margin="5" Foreground="{StaticResource MainColorGrey}" Checked="RefreshViewCheckbox" Unchecked="RefreshViewCheckbox"></CheckBox>
                                        <CheckBox x:Name="chk_drafs"  Content="Drafts" Checked="RefreshViewCheckbox" Foreground="{StaticResource MainColorGrey}" Margin="5" Unchecked="RefreshViewCheckbox"></CheckBox>-->
                                        <!--<DatePicker SelectedDateFormat="Short"  Name="dp_invoiceDate" Width="100" Margin="10,5,10,5" Padding="10,0,5,0"  Height="40" DockPanel.Dock="Right" SelectedDateChanged="RefreshView_SelectedDateChanged" BorderBrush="#737373"  BorderThickness="0" 
                                                    Style="{StaticResource MaterialDesignFloatingHintDatePicker}"  materialDesign:TextFieldAssist.CharacterCounterStyle="{Binding}"
                                                    materialDesign:HintAssist.Hint="Date..."></DatePicker>--><!--
                                    </DockPanel>-->
                                    <DockPanel Grid.Column="2" LastChildFill="False">
                                        <DatePicker SelectedDateFormat="Short"  Name="dp_invoiceDate" Width="100" Margin="10,5,10,5" Padding="10,0,5,0"  Height="40" DockPanel.Dock="Right" SelectedDateChanged="RefreshView_SelectedDateChanged" BorderBrush="#737373"  BorderThickness="0" 
                                                    Style="{StaticResource MaterialDesignFloatingHintDatePicker}"  materialDesign:TextFieldAssist.CharacterCounterStyle="{Binding}"
                                                    materialDesign:HintAssist.Hint="Date..."></DatePicker>
                                    </DockPanel>
                                    <!--Branch-->
                                    <DockPanel Grid.Row="1"  LastChildFill="False">

                                        <ComboBox SelectionChanged="cb_branches_SelectionChanged" MinWidth="150" Name="cb_branches" 
                                                  Style="{StaticResource MaterialDesignFloatingHintComboBox}"
                                                  materialDesign:HintAssist.Hint="Branch ...."
                                                  Foreground="#4E4E4E"  TextBlock.FontSize="12" Background="#f8f8f8"  BorderBrush="#737373" BorderThickness="0"  
                                                  Height="40" Margin="10,5,10,5" Padding="10,0,5,0" Grid.Row="1" KeyUp="Cb_branches_KeyUp"></ComboBox>
                                        <CheckBox DockPanel.Dock="Right" Background="{StaticResource MainColorBlue}" x:Name="chk_allBranches" Grid.Row="1"  Content="All" Style="{StaticResource MaterialDesignFilterChipPrimaryOutlineCheckBox}" Checked="Chk_allBranches_Checked" Unchecked="Chk_allBranches_Unchecked"></CheckBox>

                                    </DockPanel>
                                    <!--pos-->
                                    <DockPanel x:Name="pnl_pos" IsEnabled="False" Grid.Row="1" Grid.Column="1"  LastChildFill="False">

                                    <ComboBox MinWidth="150" Name="cb_pos" 
                                              Style="{StaticResource MaterialDesignFloatingHintComboBox}"
                                              materialDesign:HintAssist.Hint="Pos ...."
                                              Foreground="#4E4E4E"  TextBlock.FontSize="12" Background="#f8f8f8"  BorderBrush="#737373" BorderThickness="0"  
                                              Height="40" Margin="10,5,10,5" Padding="10,0,5,0" Grid.Row="1" SelectionChanged="Cb_pos_SelectionChanged" KeyUp="Cb_pos_KeyUp"></ComboBox>
                                        <CheckBox Background="{StaticResource MainColorBlue}" x:Name="chk_allPos" Grid.Row="2"  Content="All" Style="{StaticResource MaterialDesignFilterChipPrimaryOutlineCheckBox}" Checked="Chk_allPos_Checked" Unchecked="Chk_allPos_Unchecked"></CheckBox>

                                    </DockPanel>
                                    <!--Payment type-->
                                    <DockPanel Grid.Row="2"  LastChildFill="False">
                                        <!--ItemsSource="{Binding}"
                                        <ComboBox.ItemTemplate>
                                            <DataTemplate>
                                                <TextBlock Text="{Binding Path=processType, Converter={StaticResource processTypeConverter}}"/>
                                            </DataTemplate>
                                        </ComboBox.ItemTemplate>-->
                                        <ComboBox MinWidth="150" Name="cb_payments" 
                                                  Style="{StaticResource MaterialDesignFloatingHintComboBox}"
                                                  materialDesign:HintAssist.Hint="Payment Type ...."
                                                  Foreground="#4E4E4E"  TextBlock.FontSize="12" Background="#f8f8f8"  BorderBrush="#737373" BorderThickness="0"  
                                                  Height="40" Margin="10,5,10,5" Padding="10,0,5,0" Grid.Row="1" SelectionChanged="Cb_payments_SelectionChanged">
                                           
                                        </ComboBox>
                                        <CheckBox DockPanel.Dock="Right" Background="{StaticResource MainColorBlue}" x:Name="chk_allPaymentTypes" Grid.Row="1"  Content="All" Style="{StaticResource MaterialDesignFilterChipPrimaryOutlineCheckBox}" Checked="Chk_allPaymentTypes_Checked" Unchecked="Chk_allPaymentTypes_Unchecked" ></CheckBox>

                                    </DockPanel>
                                    <!--cards-->
                                    <DockPanel Grid.Row="2" Grid.Column="1"  LastChildFill="False" x:Name="pnl_cards" Visibility="Collapsed">

                                        <ComboBox MinWidth="150" Name="cb_card" 
                                                  Style="{StaticResource MaterialDesignFloatingHintComboBox}"
                                                  materialDesign:HintAssist.Hint="Card ...."
                                                  Foreground="#4E4E4E"  TextBlock.FontSize="12" Background="#f8f8f8"  BorderBrush="#737373" BorderThickness="0"  
                                                  Height="40" Margin="10,5,10,5" Padding="10,0,5,0" Grid.Row="1" SelectionChanged="Cb_card_SelectionChanged" ></ComboBox>
                                        <CheckBox DockPanel.Dock="Right" Background="{StaticResource MainColorBlue}" x:Name="chk_allCards" Grid.Row="2"  Content="All" Style="{StaticResource MaterialDesignFilterChipPrimaryOutlineCheckBox}" Checked="Chk_allCards_Checked" Unchecked="Chk_allCards_Unchecked" ></CheckBox>

                                    </DockPanel>
                                </Grid>
                                <Grid Grid.Row="1" x:Name="grid_order" Margin="10,0,10,10" Visibility="Collapsed">
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="auto"></RowDefinition>
                                        <RowDefinition Height="auto" ></RowDefinition>

                                    </Grid.RowDefinitions>
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="*"></ColumnDefinition>
                                        <ColumnDefinition Width="*"></ColumnDefinition>
                                        <ColumnDefinition Width="1.5*" MinWidth="200"></ColumnDefinition>
                                    </Grid.ColumnDefinitions>


                                    <DockPanel Grid.ColumnSpan="3" LastChildFill="False">
                                        <CheckBox x:Name="chk_orderInvoice" Content="Order" Margin="5" Foreground="{StaticResource MainColorGrey}" Checked="RefreshViewCheckbox" Unchecked="RefreshViewCheckbox"></CheckBox>
                                        <CheckBox x:Name="chk_orderDraft"  Content="Drafts" Checked="RefreshViewCheckbox" Foreground="{StaticResource MainColorGrey}" Margin="5" Unchecked="RefreshViewCheckbox"></CheckBox>
                                        <DatePicker SelectedDateFormat="Short"  Name="dp_orderDate" Width="100" Margin="10,5,10,5" Padding="10,0,5,0" Height="40" DockPanel.Dock="Right" SelectedDateChanged="RefreshView_SelectedDateChanged" BorderBrush="#737373"  BorderThickness="0" 
                                       Style="{StaticResource MaterialDesignFloatingHintDatePicker}"  materialDesign:TextFieldAssist.CharacterCounterStyle="{Binding}"
                                materialDesign:HintAssist.Hint="Date..."></DatePicker>


                                    </DockPanel>
                                </Grid>
                                <Grid Grid.Row="1" x:Name="grid_quotation" Margin="10,0,10,10"  Visibility="Collapsed">
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="auto"></RowDefinition>
                                        <RowDefinition Height="auto" ></RowDefinition>

                                    </Grid.RowDefinitions>
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="*"></ColumnDefinition>
                                        <ColumnDefinition Width="*"></ColumnDefinition>
                                        <ColumnDefinition Width="1.5*" MinWidth="200"></ColumnDefinition>
                                    </Grid.ColumnDefinitions>


                                    <DockPanel Grid.ColumnSpan="3" LastChildFill="False">
                                        <CheckBox x:Name="chk_quotationInvoice" Content="Quotation" Margin="5" Foreground="{StaticResource MainColorGrey}" Checked="RefreshViewCheckbox" Unchecked="RefreshViewCheckbox"></CheckBox>
                                        <CheckBox x:Name="chk_quotationDraft"  Content="Drafts" Checked="RefreshViewCheckbox" Foreground="{StaticResource MainColorGrey}" Margin="5" Unchecked="RefreshViewCheckbox"></CheckBox>
                                        <DatePicker SelectedDateFormat="Short"  Name="dp_quotationDate" Width="100" Margin="10,5,10,5" Padding="10,0,5,0" Height="40" DockPanel.Dock="Right" SelectedDateChanged="RefreshView_SelectedDateChanged" BorderBrush="#737373"  BorderThickness="0" 
                                       Style="{StaticResource MaterialDesignFloatingHintDatePicker}"  materialDesign:TextFieldAssist.CharacterCounterStyle="{Binding}"
                                materialDesign:HintAssist.Hint="Date..."></DatePicker>


                                    </DockPanel>


                                </Grid>
                            </Grid>

                        </ScrollViewer>
                    </Border>
                </Border>
            </Grid>
            <Border Grid.Row="1" Background="White" CornerRadius="7,7,0,0"  Margin="5,0,5,0">
                <Border BorderThickness="0,0,0,1" BorderBrush="#FFC8C8C8" Margin="5,0,5,0">
                    <DockPanel  LastChildFill="False">



