                            <DataGridTextColumn Width="*"  Header="sales man"      Binding="{Binding Path=shipUserName}"/>
                            <DataGridTextColumn Width="*"  Header="customer"       Binding="{Binding Path=agentName}"/>
                            <DataGridTextColumn Width="*"  Header="Date"           Binding="{Binding updateDate, Converter={StaticResource dateFrameConverter }}"/>
                            <DataGridTextColumn Width="*"  Header="Cash"           Binding="{Binding Path =deserved,Converter={StaticResource accuracyConverter }}" Visibility="Hidden"/>
                            <DataGridTextColumn Width="*"  Header="Status"         Binding="{Binding status,Converter={StaticResource invoiceStatusConverter }}" Visibility="Hidden"/>
                            <DataGridTextColumn Width="*"  Header="Paid"           Binding="{Binding payStatus,Converter={StaticResource invoicePayStatus }}" />
                        </DataGrid.Columns>
