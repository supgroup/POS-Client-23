                <Border BorderThickness="0"  Grid.Column="0" Grid.ColumnSpan="1" Grid.Row="2"   Height="50" Margin="10,10,5,10"
                    CornerRadius="7" BorderBrush="Black" Background="White" Padding="0" x:Name="brd_returnInvoice">
                    <Button Grid.Column="8" BorderBrush="{x:Null}"     Margin="5,0,5,0" Height="auto" 
                             x:Name="btn_returnInvoice" Click="Btn_returnInvoice_Click"
                    Background="{x:Null}"   Padding="0"    >
                        <StackPanel>
                            <Path Fill="{StaticResource MainColorBlue}" Stretch="Fill"  x:Name="path_itemsIcon1"
                         Height="25" Width="25"
                           FlowDirection="LeftToRight"  Data="M12,0C5.4,0,0,5.4,0,12s5.4,12,12,12s12-5.4,12-12S18.6,0,12,0z M15.2,18.9c5.7-5.9-0.6-10.8-5-7.7L12,13H6V7l1.8,1.8
	C15.7,2.8,24,13.3,15.2,18.9z"/>
                            <TextBlock Text="return" x:Name="txt_returnInvoice"  Foreground="{StaticResource MainColorBlue}" 
                                           FontWeight="Regular" FontSize="8" TextAlignment="Center" Width="35"
                                           HorizontalAlignment="Center"/>
                        </StackPanel>
                    </Button>
                </Border>
                    <Border BorderThickness="0"  Grid.Column="1" Grid.ColumnSpan="1" Grid.Row="2"   Height="50" Margin="10,10,5,10"
                        CornerRadius="7" BorderBrush="Black" Background="White" Padding="0" x:Name="brd_totalDescount">
                            <StackPanel Orientation="Horizontal" Margin="5,0" VerticalAlignment="Center" >

                                <TextBlock Text="Discount" x:Name="txt_totalDescount" Foreground="{StaticResource MainColorBlue}" FontWeight="Bold" 
                                           VerticalAlignment="Center" HorizontalAlignment="Left" 
                                   Margin="2,0,5,0" FontSize="22"/>
                                <TextBlock  Grid.Column="1" Margin="5,0,0,0" FontWeight="Bold" FontSize="18"  VerticalAlignment="Center"
                                       TextAlignment="Center" Name="tb_totalDescount" Foreground="#4E4E4E" HorizontalAlignment="Right" 
                                          Text="0"          />
                            </StackPanel>
                        </Border>
                    <Border BorderThickness="0"  Grid.Column="2" Grid.ColumnSpan="1" Grid.Row="2"   Height="50" Margin="5,10,5,10"
                        CornerRadius="7" BorderBrush="Black" Background="White" Padding="0" x:Name="brd_total">
                            <StackPanel Orientation="Horizontal" Margin="5,0" VerticalAlignment="Center" >

                                <TextBlock Text="TOTAL" x:Name="txt_total" Foreground="{StaticResource MainColorBlue}" FontWeight="Bold" VerticalAlignment="Center" HorizontalAlignment="Left" 
                                   Margin="2,0,5,0" FontSize="22"/>
                                <TextBlock  Grid.Column="1" Margin="5,0,0,0" FontWeight="Bold" FontSize="18"  VerticalAlignment="Center"
                                       TextAlignment="Center" Name="tb_total" Foreground="#4E4E4E" HorizontalAlignment="Right" 
                                          Text="0"          />
                                <TextBlock   Foreground="#4E4E4E" FontWeight="Bold" VerticalAlignment="Center" HorizontalAlignment="Center" 
                                   Margin="5,0,0,0" FontSize="18"
                                Grid.Row="1"   x:Name="tb_moneyIconTotal" Grid.Column="1"  
                          Text="{Binding Path=AppSettings.Currency,  Mode=OneWay}"/>

                            </StackPanel>
                        </Border>
                </Grid>
            </Grid>
        </Grid>
</UserControl>
