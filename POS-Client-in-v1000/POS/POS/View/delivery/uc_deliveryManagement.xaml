                            <ToolTip x:Name="tt_refresh" Content="Refresh"
                                              Placement="left" 
                                              HorizontalOffset="15"
                                              PlacementRectangle="50,0,0,0"
                                              VerticalOffset="25"/>
                        </Button.ToolTip>
                    </Button>
                    <StackPanel  DockPanel.Dock="Right" Orientation="Horizontal" >
                        <CheckBox x:Name="chk_drivers"   IsChecked="True"
                                                          Background="{StaticResource MainColorBlue}"
                                                          Content="drivers"
                                                          Style="{StaticResource MaterialDesignFilterChipPrimaryOutlineCheckBox}"
                                                          Checked="deliveryType_check"
                                                          Unchecked="deliveryType_uncheck"/>
                        <CheckBox x:Name="chk_shippingCompanies"  
                                                          Background="{StaticResource MainColorBlue}"
                                                          Content="shippingCompanies"
                                                          Style="{StaticResource MaterialDesignFilterChipPrimaryOutlineCheckBox}"
                                                          Checked="deliveryType_check"
                                                          Unchecked="deliveryType_uncheck"/>


                        <Border  DockPanel.Dock="Right"   Margin="7.5,10"    CornerRadius="0,0,0,0"  Width="2" BorderThickness="0" Background="#377EB8" />

                    </StackPanel>

                </DockPanel>
            </Border>

                
            <Border Margin="5,5,5,0" Grid.Row="1"  Background="White" CornerRadius="7,7,0,0">

                <Grid Margin="5,5,0,17.5">
                    <Grid.RowDefinitions>
                        <RowDefinition Height="auto"/>
                        <RowDefinition Height="*"/>
                    </Grid.RowDefinitions>
                    <Grid Grid.Row="0" Margin="5 0">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition x:Name="col_cbDriver" MaxWidth="300" Width="*"  />
                            <ColumnDefinition x:Name="col_cbCompany" MaxWidth="300"  Width="*"  />
                            <ColumnDefinition  Width="auto"  />
                        </Grid.ColumnDefinitions>

                        <Border x:Name="bdr_searchUser" Background="White" Grid.Column="0" CornerRadius="10"    Margin="0,5,5,5"  BorderThickness="0" BorderBrush="{StaticResource MainColor}">
                           
                            <Grid>
                                <ComboBox x:Name="cb_searchUser"  Grid.Row="1"
                                      Background="#f8f8f8" 
                                      Padding="10,0,5,0"  TextBlock.Foreground="#4E4E4E"
                                      Style="{StaticResource MaterialDesignFloatingHintComboBox}"
                                      materialDesign:HintAssist.Hint="delivery man..."  
                                      Grid.Column="1" Height="40"   
                                      BorderBrush="#737373"   BorderThickness="0" Margin="4,0" 
                                      SelectionChanged="Cb_searchUser_SelectionChanged" KeyUp="Cb_searchUser_KeyUp" />
                            </Grid>

                        </Border>
                        <Border  x:Name="bdr_searchCompany" Background="White" Grid.Column="1" CornerRadius="10"    Margin="0,5,5,5"  BorderThickness="0" BorderBrush="{StaticResource MainColor}">
                            <Grid>
                                <ComboBox x:Name="cb_searchCompany"  Grid.Row="1"
                                      Background="#f8f8f8" 
                                          Padding="10,0,5,0"  TextBlock.Foreground="#4E4E4E"
                                      Style="{StaticResource MaterialDesignFloatingHintComboBox}"
                                      materialDesign:HintAssist.Hint="shipping company..."  
                                      Grid.Column="1" Height="40"   
                                      BorderBrush="#737373"   BorderThickness="0" Margin="4,0" SelectionChanged="Cb_searchCompany_SelectionChanged" KeyUp="Cb_searchCompany_KeyUp"/>
                            </Grid>

                        </Border>
                        <StackPanel  Grid.Column="2"  Margin="0 0 10 0"   Orientation="Horizontal" >

                            <CheckBox x:Name="chk_allForDelivery"   
                                                          Background="{StaticResource MainColorBlue}"
                                                          Content="allForDelivery"
                                                          Style="{StaticResource MaterialDesignFilterChipPrimaryOutlineCheckBox}"
                                                          Checked="search_Checking"
                                                          Unchecked="chk_uncheck"/>

                            <CheckBox x:Name="chk_readyForDelivery"  
                                                          Background="{StaticResource MainColorBlue}"
                                                          Content="readyForDelivery"
                                                          Style="{StaticResource MaterialDesignFilterChipPrimaryOutlineCheckBox}"
                                                          Checked="search_Checking"
                                                          Unchecked="chk_uncheck"/>
                            <CheckBox x:Name="chk_withDeliveryMan"  
                                                          Background="{StaticResource MainColorBlue}"
                                                          Content="withDeliveryMan"
                                                          Style="{StaticResource MaterialDesignFilterChipPrimaryOutlineCheckBox}"
                                                          Checked="search_Checking"
                                                          Unchecked="chk_uncheck"/>
                            <CheckBox x:Name="chk_inTheWay"  
                                                          Background="{StaticResource MainColorBlue}"
                                                          Content="inTheWay"
                                                          Style="{StaticResource MaterialDesignFilterChipPrimaryOutlineCheckBox}"
                                                          Checked="search_Checking"
                                                          Unchecked="chk_uncheck"/>
                        </StackPanel>

                    </Grid>

                    <Grid  Grid.Row="1" >
                        <Border Margin="5,5,5,17.5"   Grid.Row="1" CornerRadius="10" x:Name="brd_roundDataGrid" BorderThickness="0" 
                            Background="White"/>
                        <DataGrid  Margin="5,5,5,5" Grid.Row="1"  IsReadOnly="True"   HeadersVisibility="Column"
                                   x:Name="dg_orders" AutoGenerateColumns="False"   Background="White"
                                   ColumnWidth="SizeToHeader"    AlternatingRowBackground="#f3f3f3" AlternationCount="2"
                                   SelectionChanged="Dg_orders_SelectionChanged" >
                            <DataGrid.Resources>
                                <SolidColorBrush x:Key="{x:Static SystemColors.HighlightBrushKey}"></SolidColorBrush>
                                <Style BasedOn="{StaticResource {x:Type DataGridColumnHeader}}" TargetType="{x:Type DataGridColumnHeader}">
                                    <Setter Property="Background" Value="#e7f3fa" />
                                    <Setter Property="BorderBrush" Value="White"/>
                                    <Setter Property="BorderThickness" Value="0 0 1 0"/>
                                </Style>
                            </DataGrid.Resources>
                            <DataGrid.Columns>
                                <DataGridCheckBoxColumn x:Name="col_chk" Binding="{Binding IsChecked, NotifyOnTargetUpdated=True}" 
                                                     Visibility="Collapsed"   IsReadOnly="False" IsThreeState="False">
                                    <DataGridCheckBoxColumn.ElementStyle>
                                        <Style
                                        TargetType="CheckBox">
                                            <Setter Property="FlowDirection" Value="LeftToRight"/>
                                            <Setter Property="HorizontalAlignment" Value="Center"/>
                                            <EventSetter
                                            Event="CheckBox.Checked"
                                            Handler="FieldDataGridChecked" />
                                            <EventSetter
                                            Event="CheckBox.Unchecked"
                                            Handler="FieldDataGridUnchecked" />
                                        </Style>
                                    </DataGridCheckBoxColumn.ElementStyle>
                                    <DataGridCheckBoxColumn.HeaderTemplate >
                                        <DataTemplate >
                                            <CheckBox   Checked="FieldDataGridCheckedHeader" Unchecked="FieldDataGridUncheckedHeader" Margin="2" />
                                        </DataTemplate>
                                    </DataGridCheckBoxColumn.HeaderTemplate>
                                </DataGridCheckBoxColumn>
                                <DataGridTextColumn Width="*"  Header="code"          Binding="{Binding Path=invNumber}"/>
                                <DataGridTextColumn x:Name="col_company" Visibility="Hidden" Width="*"  Header="Company"       Binding="{Binding Path=shipCompanyName , Converter={StaticResource shippingCompanyNameConverter}}"/>
                                <DataGridTextColumn x:Name="col_driver" Width="*"  Header="delivery man"    Binding="{Binding Path=shipUserName }"/>
                                <DataGridTextColumn x:Name="col_status" Width="*"  Header="status"   Binding="{Binding Path=status , Converter={StaticResource preparingOrderStatusConverter}}"/>
                             </DataGrid.Columns>
                        </DataGrid>

                    </Grid>

                </Grid>
            </Border>
            <!--#region -->
            <Border Grid.Row="2" Margin="5,0,5,0" Background="White" CornerRadius="0,0,7,7" >
                <Border BorderThickness="0,0,0,1" BorderBrush="#178DD2" Margin="10,0,10,10" Width="auto" HorizontalAlignment="Left">
                    <StackPanel  Orientation="Horizontal" >

